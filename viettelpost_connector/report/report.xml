<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="paperformat_payment_receipt_report" model="report.paperformat">
            <field name="name">Receipt/Payment Voucher</field>
            <field name="default" eval="False"/>
            <field name="disable_shrinking" eval="True"/>
            <field name="format">A5</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">5</field>
            <field name="margin_left">15</field>
            <field name="margin_right">15</field>
            <field name="header_spacing">30</field>
            <field name="dpi">96</field>
        </record>
        <record id="action_report_payment_receipt" model="ir.actions.report">
            <field name="name">Receipt/Payment Voucher</field>
            <field name="model">account.payment</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">viettelpost_connector.receipt_payment_voucher_report</field>
            <field name="report_file">viettelpost_connector.receipt_payment_voucher_report</field>
            <field name="print_report_name">(object._get_name_payment_voucher())</field>
            <field name="binding_model_id" ref="model_account_payment"/>
            <field name="binding_type">report</field>
            <field name="attachment_use">False</field>
            <field name="paperformat_id" ref="paperformat_payment_receipt_report"/>
        </record>
        <record id="action_debs_report" model="ir.actions.report">
            <field name="name">Debs Report</field>
            <field name="model">debs.report.wizard</field>
            <field name="report_type">xlsx</field>
            <field name="print_report_name">(object._get_name_report_xls())</field>
            <field name="report_name">viettelpost_connector.debs_report_xls</field>
            <field name="report_file">viettelpost_connector.debs_report_xls</field>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>