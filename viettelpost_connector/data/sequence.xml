<odoo>
    <data>
        <record id="seq_receipt_bank_voucher" model="ir.sequence">
            <field name="name">Receipt Bank Sequence</field>
            <field name="code">account.payment</field>
            <field name="prefix">PT</field>
            <field name="suffix">-%(month)s/%(y)s/CK</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_receipt_cash_voucher" model="ir.sequence">
            <field name="name">Receipt Cash Sequence</field>
            <field name="code">account.payment</field>
            <field name="prefix">PT</field>
            <field name="suffix">-%(month)s/%(y)s/TM</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_payment_bank_voucher" model="ir.sequence">
            <field name="name">Payment Bank Sequence</field>
            <field name="code">account.payment</field>
            <field name="prefix">PC</field>
            <field name="suffix">-%(month)s/%(y)s/CK</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_payment_cash_voucher" model="ir.sequence">
            <field name="name">Payment Cash Sequence</field>
            <field name="code">account.payment</field>
            <field name="prefix">PC</field>
            <field name="suffix">-%(month)s/%(y)s/TM</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
